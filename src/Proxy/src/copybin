

        WALKING_ANIMATION_SPRITE.clear();
        int old_y = 0;
        int old_x = 0;
        int frames = 1;
        int ofx=0, ofy=0;
        if(body == FRONT){
            old_y = pos_y - 32;
            ofy = 32;
            frames = 6;
        } else if (body == BACK){
            old_y = pos_y + 32;
            ofy = -32;
            frames = 6;
        } else if (body == LEFT){
            old_x = pos_x + 32;
            ofx = -32;
            frames = 4;
        } else if (body == RIGHT){
            old_x = pos_x - 32;
            ofx = 32;
            frames = 4;
        }


    for (int i = 0; i < frames ; ++i) {
        WALKING_ANIMATION_SPRITE.push_back(SDL_Rect{i * armourSpriteSheetTexture.getWidth(), body * armourSpriteSheetTexture.getHeight(),
                                    armourSpriteSheetTexture.getWidth(),armourSpriteSheetTexture.getHeight()});
    }

for (int j = 0; j < frames ; ++j) {
    }
